{
  "name": "@xico2k/react-scroll-area",
  "version": "1.1.18",
  "author": "Francisco Madeira",
  "private": false,
  "homepage": "https://xico2k.github.io/react-scroll-area/",
  "license": "MIT",
  "repository": "https://github.com/xico2k/react-scroll-area",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.4.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.3",
    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jsdom": "^16.3.0",
    "raf": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": ">=16.13.x",
    "react-dom": ">=16.13.x"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.dev.js --open",
    "build": "rm -rf docs && webpack --mode development --config webpack.config.docs.js",
    "lib": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --copy-files --ignore '**/entry.js, **/demo, **/*.test.js' && webpack --mode production",
    "test": "NODE_ENV=testing jest",
    "lint": "eslint .",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "jest": {
    "roots": [
      "src/"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}

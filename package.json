    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": ">=16.8.x",
    "react-dom": ">=16.8.x"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.config.dev.js --open",
    "build": "rm -rf docs && webpack --mode development --config webpack.config.docs.js",
    "lib": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --copy-files --ignore '**/entry.js, **/demo, **/*.test.js' && webpack --mode production",
    "test": "NODE_ENV=testing jest",
    "lint": "eslint ."
  },
  "jest": {
    "roots": [
      "src/"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  }

  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jsdom": "^11.3.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "classname": "^0.0.0",
    "prop-types": "^15.6.0",
    "react": ">= 0.13.x",
    "react-dom": ">= 0.13.x"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --open",
    "build": "rm -rf docs && NODE_ENV=development webpack --config webpack.config.docs.js",
    "lib": "rm -rf lib && NODE_ENV=production babel src --out-dir lib --copy-files --ignore '**/entry.js, **/demo, **/*.test.js' && webpack",
    "test": "NODE_ENV=testing jest",
    "lint": "eslint ."
  },
  "jest": {
    "roots": [
      "src/"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  }
